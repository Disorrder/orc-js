{
  "name": "orc-js",
  "version": "0.0.1",
  "description": "One Ridiculous Clicker with Electron",
  "main": "main.js",
  "scripts": {
    "//postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "dev": "webpack --watch",
    "build": "webpack",
    "build-win32": "npm run rebuild && build -w --ia32",
    "build-win64": "npm run rebuild && build -w --x64",
    "rebuild": "electron-rebuild -f"
  },
  "keywords": [
    "Electron",
    "Clicker",
    "Bot"
  ],
  "author": "Disordder",
  "license": "CC0-1.0",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "colors": "^1.2.4",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "electron": "^2.0.0",
    "electron-builder": "^20.15.1",
    "electron-rebuild": "^1.7.3",
    "file-loader": "^1.1.11",
    "fontgen-loader": "^0.2.1",
    "html-webpack-plugin": "^3.2.0",
    "nib": "^1.1.2",
    "open-browser-webpack-plugin": "0.0.5",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-notifier": "^1.6.0"
  },
  "dependencies": {
    "@disorrder/animated": "^0.2.0",
    "jquery": "^3.3.1",
    "robot-js": "^2.0.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.17",
    "vuex": "^3.0.1"
  },
  "build": {
    "appId": "app.id",
    "npmRebuild": false,
    "win": {
      "artifactName": "${productName}_v${version}_${arch}.${ext}",
      "target": "portable",
      "icon": "src/assets/TueEARS.png"
    }
  }
}
